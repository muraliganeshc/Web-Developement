<h1>Results Page</h1>
<ul>
    <% if(results.Response = "False") {%>
        <h2> No Movies Found</h2>
    <% } else { %>
    
    <% results.Search.forEach(function(movie){ %>
    <li> <strong><%= movie.Title %></strong> - <%= movie.Year %> </li>
    
    <% }); %> 
    <% } %>
</ul>

